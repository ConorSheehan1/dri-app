<h2><%= t('dri.views.collection.new.title') %></h2>
<div class="dri_ingest_form_container">
<%= form_for @object, as: :batch, :url => {:controller=>"collections", :action=>"create"}, :method=>"post", :html => {:class => 'dri_ingest_form'} do |f| %>

    <%= render :partial => "shared/form_metadata_qualifieddublincore", :locals => { :f => f, :action => "update" } %>

    <%= render :partial => "shared/access_controls", :locals => {:f => f,:record => @object } %>

    <%= f.submit t('dri.views.collection.buttons.create_collection'), :class => "btn-primary btn-large", :id => "create_new_collection" %>

<% end %>
</div>
